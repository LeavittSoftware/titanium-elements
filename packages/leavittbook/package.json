  "scripts": {
    "start": "concurrently --kill-others --names tsc,wds,docs  \"npm run tsc:watch\" \"web-dev-server\" \"npm run docs:watch\"",
    "tsc:watch": "tsc --watch",
    "start:build": "web-dev-server --root-dir dist --app-index index.html --open --compatibility none --port 9090",
    "build": "rimraf dist && tsc && npm run docs && rollup -c rollup.config.js",
    "lint:eslint": "eslint --ext .ts  . --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "docs": "cem analyze --globs \"../**/src/*.ts\" --exclude \"../**/node_modules/@leavittsoftware/**/src/*.ts\" --litelement",
    "docs:watch": "cem analyze --watch --globs \"../**/src/*.ts\" --exclude \"../**/node_modules/@leavittsoftware/**/src/*.ts\" --litelement",
    "clean:dev": "rimraf \"src/**/*.js.map\" \"src/**/*.js\"",
    "clean:modules": "rimraf \"./**/node_modules/!(rimraf|.bin)\" \"./package-lock.json\"",
    "clean:all": "npm run clean:dev && npm run clean:modules",
    "gen:assets": "pwa-asset-generator manifest/icon.svg manifest --opaque --icon-only --type png --manifest ./manifest.json --padding '20%'"
  },
  "customElements": "custom-elements.json"
