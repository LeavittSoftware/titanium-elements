  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run gen-docs && run-p  gen-docs:watch storybook:start",
    "storybook:start": "start-storybook -s ./static -p 6006",
    "build-storybook": "build-storybook",
    "site:build": "npm run gen-docs && build-storybook -s ./static -o ./site/titanium-elements",
    "gen-docs": "wca analyze \"../**/src/*.{ts}\" --format json --outFile custom-elements.json",
    "gen-docs:watch": "npm-watch gen-docs",
    "diagnose": "wca diagnose \"../**/src/*.{ts}\""
  },
  "watch": {
    "gen-docs": {
      "patterns": [
        "../**/src/"
      ],
      "extensions": "ts",
      "delay": 2500
    }
  },
