{
  "compilerOptions": {
    "target": "es6",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "dom"
    ],
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "declaration": false,
    "sourceMap": false,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "allowJs": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true
  },
  "include": [
    "*.ts"
  ]
}