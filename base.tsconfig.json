{
  "compilerOptions": {
    "target": "es2022",
    "module": "esNext",
    "moduleResolution": "node",
    "declarationMap": true,
    "lib": ["esnext", "dom", "dom.iterable"],
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "useDefineForClassFields": false,
    "declaration": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "allowJs": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "skipLibCheck": true
  }
}
